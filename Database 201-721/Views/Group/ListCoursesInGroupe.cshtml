@model Database_201_721.ViewModels.GroupAndCourseViewModel

<h2>Список предметов в группе</h2>
<table class="table">
    @foreach (var course in @Model.Courses)
    {
        <tr>
            <td> @course.CourseName </td>
        </tr>
    }
</table>

<a asp-action="AddCoursesInGroup" asp-route-id = "@Model.Group.Id">Добавить курсы в группу</a>



